\newlabel{frm:5}{{4.6.2}{26}{Discounted cashflow}{subsubsection.4.6.2}{}}
\addformulariumentry{0}{Formule Huidige Waarde (HW)}{ HW = \DOTSB \sum@ \slimits@ _{k=0}^{n} \frac  {R_k-E_k}{(1 + i)^k} = \DOTSB \sum@ \slimits@ _{k=0}^{n} \frac  {CF_k}{(1 + i)^k} }{met $HW$ de huidige waarde, $R_k$ de inkomsten in periode $k$, $E_k$ de uitgaven in periode $k$, $i$ de discontovoet, $CF_k$ de cashflow in periode $k$, en $n$ de hoeveelheid periodes.}{5}
\newlabel{frm:6}{{4.6.2}{26}{Discounted cashflow}{subsubsection.4.6.2}{}}
\addformulariumentry{0}{Formule Netto Huidige Waarde (NHW)}{ NHW = HW - I_0 = \DOTSB \sum@ \slimits@ _{k=0}^{n} \frac  {CF_k}{(1 + i)^k} - I_0 }{met $NHW$ de netto huidige waarde, $HW$ de huidige waarde, $I_0$ de initiële investering, $CF_k$ de cashflow in periode $k$, $i$ de discontovoet, en $n$ de hoeveelheid periodes.}{6}
\newlabel{frm:7}{{4.6.2}{26}{Discounted cashflow}{subsubsection.4.6.2}{}}
\addformulariumentry{0}{Formule Profitability Index (PI)}{ PI_2 = \frac  {NHW}{I_0} => PI_1 = \frac  {HW}{I_0} => PI_2 = PI_1 -1 }{met $PI$ de profitability index, $NHW$ de netto huidige waarde, $HW$ de huidige waarde, $I_0$ de initiële investering.}{7}
\newlabel{frm:8}{{4.6.2}{26}{Discounted cashflow}{subsubsection.4.6.2}{}}
\addformulariumentry{0}{Formule Internal Rate of Return (IRR)}{ 0 = \DOTSB \sum@ \slimits@ _{k=0}^{n} \frac  {CF_k}{(1 + IRR)^k} - I_0 }{met $IRR$ de internal rate of return, $CF_k$ de cashflow in periode $k$, $I_0$ de initiële investering, en $n$ de hoeveelheid periodes.}{8}
