{
  "latex-workshop.linting.enabled": true,
  "latex-workshop.linting.ignore": [
    "**/*.aux",
    "**/*.toc",
    "**/*.out",
    "**/*.fls",
    "**/*.fdb_latexmk",
    "**/*.synctex.gz",
    "**/*.log",
    "**/Thumbs.db"
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-interaction=nonstopmode",
        "-file-line-error",
        "-synctex=1",
        "%DOCFILE%"
      ],
      "env": {}
    },
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-pdf",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-synctex=1",
        "%DOCFILE%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
        "%DOCFILE%"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "pdflatex Ã— 2 (fast)",
      "tools": [
        "pdflatex",
        "pdflatex"
      ]
    },
    {
      "name": "latexmk (pdflatex)",
      "tools": [
        "latexmk"
      ]
    },
    {
      "name": "pdflatex",
      "tools": [
        "pdflatex"
      ]
    }
  ],
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.message.error.show": true,
  "latex-workshop.message.warning.show": true,
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.fdb_latexmk",
    "*.dpth",
    "*.md5",
    "*.auxlock"
  ]
}
